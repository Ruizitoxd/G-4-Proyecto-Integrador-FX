<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane id="AnchorPane" prefHeight="1080.0" prefWidth="1920.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23" fx:controller="controlador.VistaPrincipalController">
    <stylesheets>
        <URL value="@vistaprincipal.css" />
    </stylesheets>
   <children>
      <TabPane id="tabPaneBotones" fx:id="tabPaneBotones" prefHeight="1080.0" prefWidth="1920.0" styleClass="tabPane" stylesheets="@vistaprincipal.css" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab id="tabPaneChaux" fx:id="tabChaux" text="Chaux - Construction Group">
               <content>
                  <Pane prefHeight="200.0" prefWidth="200.0" />
               </content>
               <graphic>
                  <ImageView fitHeight="150.0" fitWidth="47.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../recursos/imagenes/Logo%20Chaux.png" />
                     </image>
                  </ImageView>
               </graphic>
          </Tab>
          <Tab fx:id="tabProyectos" text="Proyectos">
               <content>
                  <Pane prefHeight="200.0" prefWidth="200.0" />
               </content>
          </Tab>
            <Tab fx:id="tabVentas" text="Ventas">
               <content>
                  <Pane prefHeight="200.0" prefWidth="200.0" />
               </content>
            </Tab>
            <Tab fx:id="tabDashboard" text="Dashboard">
               <content>
                  <Pane prefHeight="200.0" prefWidth="200.0" />
               </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
